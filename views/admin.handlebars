
<h4>Welcome to Admin View {{user.firstName}}</h4>
<br>

<h4>All Users:</h4>
{{#each allUsers}}
    <hr>
    <h5>username: {{username}}</h5>
    <h6>{{firstName}} {{lastName}}</h6>
    <h6>{{email}}</h6>
    <h6>Admin: {{adminster}}</h6>
    <h6 class="valign-wrapper">Delete Account:<a class="modal-trigger" href="#modal2" info-user='{{username}}'><i class="material-icons" id="cancel-icon">cancel</i></a></h6>
{{/each}}


<!-- Modal Structure -->
  <div id="modal2" class="modal">
    <div class="modal-content">
      <h4>Are you sure you want to delete this account?</h4>
      <div class='row'>
          <a class="waves-effect waves-light btn" href='#'>Yes</a>
          <a class="waves-effect waves-light btn modal-close" href="#!">No</a>
      </div>
    </div>
  </div>
